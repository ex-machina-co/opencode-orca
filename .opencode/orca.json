{
  "$schema": "./orca.schema.json",
  "settings": {
    "defaultSupervised": false,
    "defaultModel": "anthropic/claude-opus-4-5",
    "validation": { "maxRetries": 3, "wrapPlainText": true }
  },
  "agents": {
    "executor": { "model": "anthropic/claude-opus-4-5" },
    "strategist": { "mode": "subagent" },
    "github": { "model": "openai/gpt-5.2", "reasoningEffort": "medium", "specialist": true },
    "architect": { "model": "anthropic/claude-opus-4-5" },
    "coder": { "model": "anthropic/claude-opus-4-5" },
    "product-manager": { "specialist": true }
  }
}
